<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<fx:root type="javafx.scene.layout.AnchorPane" minHeight="41.0" prefHeight="40.0" prefWidth="200.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <children>
    <Rectangle fx:id="musicSegmentRectangle" arcHeight="0.0" arcWidth="0.0" fill="#3385ff" height="40.0" layoutY="0.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.5" width="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
    <VBox prefHeight="41.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
        <Label fx:id="musicSegmentName" text="Label" textFill="#404040">
          <VBox.margin>
            <Insets left="2.0" top="2.0" />
          </VBox.margin>
        </Label>
        <HBox id="HBoxActions" fx:id="musicSegmentActions" prefHeight="100.0" prefWidth="200.0" visible="false">
          <children>
            <Button minHeight="16.0" minWidth="36.0" mnemonicParsing="false" prefHeight="16.0" prefWidth="16.0" text="Button">
              <graphic>
                <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                    <Image url="@icons/PNG/16px/play3.png" />
                  </image>
                </ImageView>
              </graphic>
              <HBox.margin>
                <Insets left="2.0" fx:id="x1" />
              </HBox.margin>
            </Button>
            <Button minHeight="16.0" minWidth="36.0" mnemonicParsing="false" prefHeight="16.0" prefWidth="16.0" text="Button" HBox.margin="$x1">
              <graphic>
                <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                    <Image url="@icons/PNG/16px/stop2.png" />
                  </image>
                </ImageView>
              </graphic>
            </Button>
            <Button minHeight="16.0" minWidth="36.0" mnemonicParsing="false" prefHeight="16.0" prefWidth="16.0" text="Button" HBox.margin="$x1">
              <graphic>
                <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                    <Image url="@icons/PNG/16px/remove2.png" />
                  </image>
                </ImageView>
              </graphic>
            </Button>
          </children>
        </HBox>
      </children>
    </VBox>
  </children>
</fx:root>
